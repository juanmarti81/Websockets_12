<h2>Lista de Productos</h2>
<br>
<a href="/" style="font-weight: bold ;"><button>Agregar Producto</button></a>
<br><br>
<ul>
  {{#each product}}
  <li>
    <h2>{{this.title}}</h2>
    <img src={{this.thumbnail}} style="height: 50px ;" />
    <br>
    <p>Precio: {{this.price}}</p>
  </li>
  <hr/>
  {{/each}}
</ul>